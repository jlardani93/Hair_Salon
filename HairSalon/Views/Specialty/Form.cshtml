@{
  Layout = "_Layout";
}

<div class="container">
  <div class="row">
    <div class="col-md-12">
      <form action="/Specialty/Create" method="post">
        <div class="form-group">
          <label for="name">Specialty Name: </label>
          <input type="text" name="name" id="name">
        </div>
        <button type="submit" class="btn">Create Specialty</button>
      </form>
      <a href="/Specialty/Info"><button type="button" class="btn">View All Specialties</button></a>
    </div>
  </div>
</div>
